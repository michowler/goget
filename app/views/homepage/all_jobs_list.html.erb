<h1>All jobs created in marketplace</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Job Pickup Address</th>
      <th>Job Dropoff Address</th>
      <th>Status</th>      
      <th>Action</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <%@job_list.each do |j|%>
    <tr>
      <td><%= j.pickup_address %></td>
      <td><%= j.dropoff_address %></td>
      <td><%= "NOT BOOKED" if j.book_status? ==false%><%= "BOOKED" if j.book_status? == true%></td>          
      <td>
        <% if j.book_status? == false %>           
          <%= link_to('Claim Job', book_job_path(j), method: :put) %>
        <% end %>         
      </td>       
    </tr>
    <%end%>
  </tbody>
</table>